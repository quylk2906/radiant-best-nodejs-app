{{#contentFor "pageStyle"}}
<!-- Select-Boxes CSS -->
<link href="/assets/css/components/select-boxes.css" rel="stylesheet" type="text/css"/>
<link href="/assets/js/summernote-0.8.2/summernote.css" rel="stylesheet">
<style type="text/css" media="screen">
    p {
     margin-bottom: 0px;
    }
</style>
    {{/contentFor}}
    <section id="content">
        <div class="content-wrap">
            <div class="container clearfix">
                <div class="heading-block center">
                    <h1>
                        This is a Blank Page
                    </h1>
                </div>
                <div class="col-md-8 col-md-offset-2 widget">
                <!-- <div class=" clearfix"> -->
                     <div class="my-contact-widget" data-loader="button">
                          <div class="contact-form-result"></div>
                            <form action="/admin/add-product" class="nobottommargin" method="post" name="template-contactform">
                                <div class="col_full">
                                    <label for="link_item">
                                        Link
                                    </label>
                                    <input class="typeahead sm-form-control tt-input required" id="" name="link_item" type="text" value=""/>
                                </div>
                                <div class="col_half">
                                    <label for="name">
                                        name
                                    </label>
                                    <input class="typeahead sm-form-control tt-input required" id="" name="name" type="text" value=""/>
                                </div>
                                <div class="col_half col_last">
                                    <label for="title_meta">
                                        title_meta
                                    </label>
                                    <input class="typeahead sm-form-control tt-input required" id="" name="title_meta" type="text" value=""/>
                                </div>
                                <div class="col_half">
                                    <label for="category">
                                        category
                                    </label>
                                    <select class="select-1 form-control" multiple="multiple" style="width:100%;" name="category">
                                        <option value="html">
                                            HTML
                                        </option>
                                        <option value="wordpress">
                                            WORDPRESS
                                        </option>
                                    </select>
                                </div>
                                <div class="col_half col_last">
                                    <label for="group">
                                        group
                                    </label>
                                    <select class="select-1 form-control" multiple="multiple" style="width:100%;" name="group">
                                        <option value="ecommerce">
                                            eCommerce
                                        </option>
                                        <option value="admin">
                                            Admin
                                        </option>
                                        <option value="real-estate">
                                            Real Estate
                                        </option>
                                        <option value="multi-purpose">
                                            Multi-purpose
                                        </option>
                                        <option value="landing">
                                            Landing
                                        </option>
                                        <option value="blog">
                                            Blog
                                        </option>
                                        <option value="service">
                                            Service
                                        </option>
                                        <option value="education">
                                            Education
                                        </option>
                                        <option value="personal">
                                            Personal
                                        </option>
                                        <option value="news">
                                            News
                                        </option>
                                        <option value="other">
                                            Other
                                        </option>
                                    </select>
                                </div>
                                <div class="col_half">
                                    <label for="price">
                                        price
                                    </label>
                                    <input class="typeahead sm-form-control tt-input required" id="" name="price" type="text" value=""/>
                                </div>
                                <div class="col_half col_last">
                                    <label for="sku">
                                        sku
                                    </label>
                                    <input class="typeahead sm-form-control tt-input required" id="" name="sku" type="text" value=""/>
                                </div>
                                <div class="col_half">
                                    <label for="sale">
                                        sale
                                    </label>
                                    <input class="typeahead sm-form-control tt-input required" id="" name="sale" type="text" value=""/>
                                </div>
                                <div class="col_half">
                                    <label for="rate">
                                        rate
                                    </label>
                                    <input class="typeahead sm-form-control tt-input required" id="" name="rate" type="text" value=""/>
                                </div>
                                <div class="col_half col_last">
                                    <label for="review">
                                        review
                                    </label>
                                    <input class="typeahead sm-form-control tt-input required" id="" name="review" type="text" value=""/>
                                </div>
                                <div class="col_half">
                                    <label for="image">
                                        image
                                    </label>
                                    <input class="typeahead sm-form-control tt-input required" id="" name="image" type="text" value=""/>
                                </div>
                                <div class="col_half col_last">
                                    <label for="thumbnail">
                                        thumbnail
                                    </label>
                                    <input class="typeahead sm-form-control tt-input required" id="" name="thumbnail" type="text" value=""/>
                                </div>
                                <div class="col_full">
                                    <label for="description">
                                        Descriptions
                                        <small>
                                            *
                                        </small>
                                    </label>
                                    <textarea class="" id="summernote" name="description"></textarea>
                                 <!--   <textarea class="typeahead sm-form-control tt-input required valid" name="template-contactform-message" rows="4" cols="30" placeholder="Enter your Message" aria-required="true" aria-invalid="false"></textarea>-->
                                </div>
                                <a class="btn btn-default topmargin-sm" href="index.html">
                                    ‚áê Back to Admin
                                </a>
                                <button class="btn btn-success topmargin-sm" type="submit">
                                    Publish
                                </button>
                            </form>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- #content end -->
    {{#contentFor "pageScripts"}}
    <script src="/assets/js/components/select-boxes.js" type="text/javascript"></script>
    <script src="/assets/js/components/selectsplitter.js" type="text/javascript"></script>
    <script src="/assets/js/summernote-0.8.2/summernote.0.8.2.min.js"></script>
</link>
<script>
    jQuery(document).ready( function($){
        $('#summernote').summernote({
            height: 200,                 // set editor height
            // focus: true                 // set focus to editable area after initializing summernote
        });
        $(".select-1").select2({
            placeholder: "Select Values",
            height: '500px'
        });
            // var draft = $('#summernote').summernote('code');
    })
</script>
{{/contentFor}}
