<section id="content">
    <div class="content-wrap">
        <div class="container clearfix">
            <div class="row clearfix">
                <div class="col-sm-9">
                    <img alt="Avatar" class="alignleft img-circle img-thumbnail notopmargin nobottommargin" src="/assets/images/icons/avatar.jpg" style="max-width: 84px;">
                        <div class="heading-block noborder">
                            <h3 id="fullname">
                                {{fullname}}
                            </h3>
                            <span>
                                Privilege: {{roles}}
                            </span>
                        </div>
                        <div class="clear">
                        </div>
                        <div class="row clearfix">
                            <div class="col-md-12">
                                <div class="tabs tabs-alt clearfix" id="tabs-profile">
                                    <ul class="tab-nav clearfix">
                                        <li>
                                            <a href="#tab-feeds">
                                                <i class="icon-rss2">
                                                </i>
                                                Your info
                                            </a>
                                        </li>
                                    </ul>
                                    <div class="tab-container">
                                        <div class="tab-content clearfix widget" id="tab-feeds">
                                            <div class="my-contact-widget" data-loader="button">
                                                <div class="contact-form-result">
                                                </div>
                                                <form action="/users/change-password" class="nobottommargin" method="post" name="template-contactform1">
                                                    <div class="col_half">
                                                        <label for="register_form_fullname">
                                                            Full name
                                                        </label>
                                                        <input class="typeahead sm-form-control tt-input required" id="" name="register_form_fullname" type="text" value=""/>
                                                        <span class="label label-danger hide">
                                                        </span>
                                                    </div>
                                                    <div class="col_half">
                                                        <label for="register_form_phone">
                                                            Phone
                                                        </label>
                                                        <input class="typeahead sm-form-control tt-input required" id="" maxlength="11" name="register_form_phone" onkeypress="return event.charCode >= 48 && event.charCode <= 57" type="text" value=""/>
                                                        <span class="label label-danger hide">
                                                        </span>
                                                    </div>
                                                    <div class="col_half">
                                                        <label for="register_form_password">
                                                            New password
                                                        </label>
                                                        <input class="typeahead sm-form-control tt-input" id="" name="register_form_password" type="password" value=""/>
                                                        <span class="label label-danger hide">
                                                        </span>
                                                    </div>
                                                    <div class="col_half col_last">
                                                        <label for="register_form_confirm_password">
                                                            Confirm new password
                                                        </label>
                                                        <input class="typeahead sm-form-control tt-input" id="" name="register_form_confirm_password" type="password" value=""/>
                                                        <span class="label label-danger hide">
                                                        </span>
                                                    </div>
                                                    <div class="col_half">
                                                        <label for="form_old_password">
                                                            Old password<small> *</small>
                                                        </label>
                                                        <input class="typeahead sm-form-control tt-input required" id="" name="form_old_password" type="password" value=""/>
                                                    </div>
                                                    <div class="col_full">
                                                    </div>
                                                    <a class="button button-rounded button-reveal button-mini button-dark" href="/users/profile">
                                                        <i class="icon-line-arrow-left">
                                                        </i>
                                                        <span>
                                                            Back to profile
                                                        </span>
                                                    </a>
                                                    <button class="button button-rounded button-reveal button-mini button-green tright" style="top: -15px" type="submit">
                                                        <i class="icon-line-arrow-right">
                                                        </i>
                                                        <span>
                                                            Update
                                                        </span>
                                                    </button>
                                                </form>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </img>
                </div>
                <div class="line visible-xs-block">
                </div>
                <div class="col-sm-3 clearfix">
                    <div class="list-group">
                        <a class="list-group-item clearfix" href="/users/profile">
                            Profile
                            <i class="icon-user pull-right">
                            </i>
                        </a>
                        <a class="list-group-item clearfix" href="#">
                            Servers
                            <i class="icon-laptop2 pull-right">
                            </i>
                        </a>
                        <a class="list-group-item clearfix" href="#">
                            Messages
                            <i class="icon-envelope2 pull-right">
                            </i>
                        </a>
                        <a class="list-group-item clearfix" href="#">
                            Billing
                            <i class="icon-credit-cards pull-right">
                            </i>
                        </a>
                        <a class="list-group-item clearfix" href="#">
                            Settings
                            <i class="icon-cog pull-right">
                            </i>
                        </a>
                        <a class="list-group-item clearfix" href="/users/logout">
                            Logout
                            <i class="icon-line2-logout pull-right">
                            </i>
                        </a>
                    </div>
                    <div class="fancy-title topmargin title-border">
                        <h4>
                            About Me
                        </h4>
                    </div>
                    <p>
                        Lorem ipsum dolor sit amet, consectetur adipisicing elit. Ipsum laboriosam, dignissimos veniam obcaecati.
                        Quasi eaque, odio assumenda porro explicabo laborum!
                    </p>
                    <div class="fancy-title topmargin title-border">
                        <h4>
                            Social Profiles
                        </h4>
                    </div>
                    <a class="social-icon si-facebook si-small si-rounded si-light" href="#" title="Facebook">
                        <i class="icon-facebook">
                        </i>
                        <i class="icon-facebook">
                        </i>
                    </a>
                    <a class="social-icon si-gplus si-small si-rounded si-light" href="#" title="Google+">
                        <i class="icon-gplus">
                        </i>
                        <i class="icon-gplus">
                        </i>
                    </a>
                    <a class="social-icon si-dribbble si-small si-rounded si-light" href="#" title="Dribbble">
                        <i class="icon-dribbble">
                        </i>
                        <i class="icon-dribbble">
                        </i>
                    </a>
                    <a class="social-icon si-flickr si-small si-rounded si-light" href="#" title="Flickr">
                        <i class="icon-flickr">
                        </i>
                        <i class="icon-flickr">
                        </i>
                    </a>
                    <a class="social-icon si-linkedin si-small si-rounded si-light" href="#" title="LinkedIn">
                        <i class="icon-linkedin">
                        </i>
                        <i class="icon-linkedin">
                        </i>
                    </a>
                    <a class="social-icon si-twitter si-small si-rounded si-light" href="#" title="Twitter">
                        <i class="icon-twitter">
                        </i>
                        <i class="icon-twitter">
                        </i>
                    </a>
                </div>
            </div>
        </div>
    </div>
</section>
<!-- #content end -->
{{#contentFor "pageScripts"}}
<script src="/assets/js/customjs/loginController.js" type="text/javascript">
</script>
{{/contentFor}}
